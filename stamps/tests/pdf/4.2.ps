%%+: ProcSet (si) 2.2 0
%%+: ProcSet (graphics-alignments) 2.2 0
%%+: ProcSet (distiller) 2.2 0
%%Pages: 1
%%PageOrder: Ascend
%%EndComments
%%BeginProlog

%%IncludeResource: ProcSet si 2.2 0
%%IncludeResource: ProcSet graphics-alignments 2.2 0
%%IncludeResource: ProcSet csm-marks 2.2 0
%%IncludeResource: ProcSet distiller 2.2 0

/mm  /si /ProcSet findresource  1 index  get  def

/range     /arrays    /ProcSet findresource  1 index  get  def

/months    /csm-marks /ProcSet findresource  1 index  get  def
/quarters  /csm-marks /ProcSet findresource  1 index  get  def
/year      /csm-marks /ProcSet findresource  1 index  get  def

%%EndProlog

%%BeginSetup

/.setpdfwrite where { pop .setpdfwrite } if
/distiller /ProcSet findresource /setdistillerparamsforcsmmarks get exec

%%EndSetup

%%Page: A 1
%%BeginSetup
%%BeginFeature: *PageSize A5
<<
  % /HWResolution [2400 2400]
  % /PageSize [ 210 mm  297 2 div mm ]
  % /ImagingBBox [ 5 mm  5 mm  210 5 sub mm  297 2 div 5 sub mm ]
  /PageSize [ 297 2 div mm  210 mm ]
  /ImagingBBox [ 5 mm  8 mm  297 2 div 5 sub mm  210 5 sub mm ]
  /MirrorPrint false
  /NegativePrint true
>> setpagedevice
%%EndFeature:
%%EndSetup
%!

gsave

%------------------------------------------------------------------------------
% тестируем генерацию знака поверки для государственных научных метрологических
% институтов
%
% для подготовки оттисков править код ниже !!!
%------------------------------------------------------------------------------
  [
      2018                                                      % год
      [ [ 1 12 range ] months  [ 1 4 range ] quarters  year ]   % периоды
      (А)                                                       % шифр поверительного клейма
      [ 1 ]                                                     % диапазон знаков поверителей
      18.0 mm                                                   % размер клейма
      false                                                     % поверка в эксплуатации или после ремонта
      /verification_stamp_upath
    /csm-marks /ProcSet findresource /create_stamps get exec
  ]
  /boxes /ProcSet findresource /GetPageBBox get exec
  2 mm
/graphics-alignments /ProcSet findresource /align_upaths_in_BBox get exec

%------------------------------------------------------------------------------
% конец области для кода генерации оттисков
%------------------------------------------------------------------------------

grestore

showpage

